<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
  <Document>
    <name>Polygon Sample</name>
    <description>This is a sample KML file with a Polygon.</description>
    <Placemark>
      <name>Example Polygon</name>
      <description>A simple polygon example.</description>
      <Polygon>
        <outerBoundaryIs>
          <LinearRing>
            <coordinates>
              139.0,35.0,0   <!-- 東京 -->
              139.1,35.0,0   <!-- 東京都内の別地点 -->
              139.1,35.9,0   <!-- 東京都内の別地点 -->
              139.0,35.9,0   <!-- 東京都内の別地点 -->
              139.0,35.0,0   <!-- 戻る -->
            </coordinates>
          </LinearRing>
        </outerBoundaryIs>
      </Polygon>
    </Placemark>
  </Document>
</kml>
